#!/bin/sh
# postrm script for aqemu

set -e

case "$1" in
    remove|purge|disappear)
        # update menu entries
        if test -x /usr/bin/update-menus; then 
            update-menus
        fi
    ;;
    upgrade|failed-upgrade|abort-upgrade|abort-install)
    ;;

    *)
        echo "postrm called with unknown argument \`$1'" >&2
        exit 1
    ;;
esac

#DEBHELPER#

exit 0

